


# To display on line graph 
import matplotlib.pyplot as plt 

years= ['2012 , '2013' , '2014', '2015','2016','2017']
profits= [9,10,11,13,15,19]


#create a line graph 
plt.plot(years, profits, 'blue')

#set company title
plt.title('WIPRO ')

plt.xlabel (' years')
plt.ylabel('Profit in millions rs')


#display the pie chart  
plt.show()





